// the regfile for all game information
module regfile (
    input clk,  // clock
    input rst,  // reset
    
    input player_position_in[16],
    output player_position_out[16]

  ) 

{
  .clk(clk){
    .rst(rst){
      dff player_position[16];
    }
  }

  always {
      player_position.d = player_position_in;
      player_position_out = player_position.q;
  }
}
